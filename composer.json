{
  "name": "adamczykpiotr/laravel-make-transformer",
  "description": "Laravel 7.x package to add artisan method `make:transformer ClassTransformer`",
  "keywords": [
    "adamczykpiotr",
	  "make-transformer",
    "laravel",
    "laravel-7",
    "laravel-8",
    "laravel-framework",
    "fractal",
    "transformer",
    "artisan",
    "api",
	  "metricloop"
  ],
  "homepage": "https://github.com/adamczykpiotr/laravel-make-transformer",
  "license": "MIT",
  "replace": {
    "metricloop/laravel-transformer-maker":"1.*"
  },
  "authors": [
    {
      "name": "Piotr Adamczyk",
      "email": "kontakt@adamczykpiotr.com",
      "homepage": "https://adamczykpiotr.com",
      "role": "Developer"
    },
    {
      "name": "Patrick Guevara",
      "email": "pguevara@metricloop.com",
      "homepage": "https://metricloop.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "AdamczykPiotr\\MakeTransformer\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdamczykPiotr\\MakeTransformer\\TransformerMakerServiceProvider"
      ]
    }
  }
}
